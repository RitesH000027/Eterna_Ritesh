    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.24",
    "@types/uuid": "^9.0.7",
